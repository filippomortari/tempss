<CompressibleFlowSolver>
    <Physics>
        <EquationSystem>
            <EulerCFE></EulerCFE>
        </EquationSystem>
        <Gamma>1.4</Gamma>
        <GasConstant>287.058</GasConstant>
        <AngleOfAttack>0</AngleOfAttack>
        <rhoInf>1.225</rhoInf>
        <pInf>101325</pInf>
        <MachInf>0.1</MachInf>
    </Physics>
    <ProblemSpecification>
        <GeometryAndBoundaryConditions>
            <Geometry></Geometry>
            <BoundaryCondition>
                <Comment></Comment>
                <u>
                    <Type></Type>
                    <Value></Value>
                    <UserDefinedType></UserDefinedType>
                </u>
                <v>
                    <Type></Type>
                    <Value></Value>
                    <UserDefinedType></UserDefinedType>
                </v>
                <p>
                    <Type></Type>
                    <Value></Value>
                    <UserDefinedType></UserDefinedType>
                </p>
            </BoundaryCondition>
        </GeometryAndBoundaryConditions>
        <Expansion>
            <PolynomialOrder>5</PolynomialOrder>
            <BasisType>MODIFIED</BasisType>
        </Expansion>
        <FinalTime>2</FinalTime>
        <TimeStep>0.0001</TimeStep>
        <CFL>0.1</CFL>
        <RiemannSolver>ExactToro</RiemannSolver>
    </ProblemSpecification>
    <NumericalAlgorithm>
        <Projection>DiscontinuousGalerkin</Projection>
        <TimeIntegration>
            <TimeIntegrationMethod>ClassicalRungeKutta4</TimeIntegrationMethod>
            <DiffusionAdvancement>Explicit</DiffusionAdvancement>
        </TimeIntegration>
        <MatrixInversion>
            <Direct>
                <SubStructuring>StaticCondensation</SubStructuring>
            </Direct>
        </MatrixInversion>
        <GlobalOptimizationParameters>
            <BackwardTransform>
                <ElementalEvaluation>
                    <Triangles>SumFactorization</Triangles>
                    <Quadrilaterals>SumFactorization</Quadrilaterals>
                </ElementalEvaluation>
            </BackwardTransform>
            <InnerProductWRTBase>
                <ElementalEvaluation>
                    <Triangles>SumFactorization</Triangles>
                    <Quadrilaterals>SumFactorization</Quadrilaterals>
                </ElementalEvaluation>
            </InnerProductWRTBase>
            <MassMatrixOp>
                <ElementalEvaluation>
                    <Triangles>SumFactorization</Triangles>
                    <Quadrilaterals>SumFactorization</Quadrilaterals>
                </ElementalEvaluation>
            </MassMatrixOp>
            <HelmholtzMatrixOp>
                <ElementalEvaluation>
                    <Triangles>SumFactorization</Triangles>
                    <Quadrilaterals>SumFactorization</Quadrilaterals>
                </ElementalEvaluation>
            </HelmholtzMatrixOp>
        </GlobalOptimizationParameters>
    </NumericalAlgorithm>
    <Admin>
        <IO_CheckSteps>1</IO_CheckSteps>
        <IO_InfoSteps>1</IO_InfoSteps>
    </Admin>
</CompressibleFlowSolver>

